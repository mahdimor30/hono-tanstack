{
  "name": "tanstack-router-simple",
  "version": "0.0.0",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "dev": "vite dev --port 3000",
    "build": "bun run build.client && bun run build.server",
    
    "build.client": "bunx --bun  vite build --outDir dist/client --ssrManifest",
    "build.server": "bunx --bun  vite build --outDir dist/server --ssr ./src/entry-server.tsx ",
    "start": "bun --bun dist/server.js --port 3000",
    "typecheck": "tsc"
  },
  "dependencies": {
    "@hono/node-server": "^1.7.0",
    "@radix-ui/react-label": "^2.1.0",
    "@ssrx/plugin-tanstack-router": "latest",
    "@ssrx/react": "latest",
    "@ssrx/vite": "latest",
    "@tanstack/react-form": "^0.29.2",
    "@tanstack/react-query": "^5.51.23",
    "@tanstack/react-router": "1.32.0",
    "@tanstack/router-devtools": "1.32.0",
    "@tanstack/router-vite-plugin": "1.31.18",
    "axios": "^1.7.4",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "hono": "3.12.10",
    "jsesc": "3.0.2",
    "lucide-react": "^0.427.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "tailwind-merge": "^2.5.2",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@hono/zod-validator": "^0.2.2",
    "@tanstack/eslint-plugin-query": "^5.51.15",
    "@types/react": "18.2.51",
    "@types/react-dom": "18.2.18",
    "@vitejs/plugin-react": "4.2.1",
    "autoprefixer": "10.4.17",
    "postcss": "8.4.33",
    "tailwindcss": "3.3.5",
    "typescript": "5.3.3",
    "vite": "5.0.12",
    "vite-tsconfig-paths": "4.3.1"
  }
}
